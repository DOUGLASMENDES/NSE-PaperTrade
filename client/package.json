    "build:css": "postcss src/tailwind.css -o public/tailwind.css",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.6.4",
    "d3": "^7.0.1",
