{
  "name": "papertrade-client",
  "version": "0.1.1",
  "private": true,
  "description": "Papper Trade Client using VUE.",
  "scripts": {
    "dev": "vue-cli-service serve --fix",
    "devcss": "postcss src/tailwind.css -o public/tailwind.css",
